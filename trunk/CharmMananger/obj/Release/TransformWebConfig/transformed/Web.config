  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <container>
      <!-- Sql适配器目前只支持sqlserver -->
      <!-- Session适配器，是使用缓存还是使用系统session,或cookie 实现类： CacheSessionProvider/SysSessionProvider/CookieSessionProvider -->
